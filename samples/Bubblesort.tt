templates bubblesort
  templates bubble
    @: 1;
    1..$-1 -> #
    $@ !
    <?($@bubblesort($+1) <..~$@bubblesort($)>)>
      @: $;
      def temp: $@bubblesort($@);
      @bubblesort($@): $@bubblesort($@+1);
      @bubblesort($@+1): $temp;
  end bubble

  @: $;
  $::length -> #
  $@ !

  <2..>
    $ -> bubble -> #
end bubblesort

[4,5,3,8,1,2,6,7,9,8,5] -> bubblesort -> !OUT::write

