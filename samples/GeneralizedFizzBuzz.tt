def input: {N: 110, words: [ { mod: 3, word: 'Fizz' }, { mod: 5, word: 'Buzz'}, {mod:7, word: 'Baxx'}]}

templates sayWords
  def i: $it
  templates maybeSay
    def word: $it.word
    $i mod $it.mod -> (<0> $word !) !
  end maybeSay
  $input.words... -> maybeSay !
end sayWords

[ 1..$input.N -> '$sayWords;' ] -> [i](<''> $i ! <> $it !) -> '$it;
' -> !OUT::write
